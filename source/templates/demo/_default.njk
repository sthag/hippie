<!-- template-default.njk -->
<!DOCTYPE html>
<html lang="de">
<head>
  <meta charset="utf-8">

  {% block head %}
  <title>{% block title %}{% endblock %} - HIPPIE</title>

  {% include "demo/partials/_meta.njk" %}

  {% block link %}{% endblock %}

  <link rel="stylesheet" type="text/css" media="all" href="/css/demo.css"/>
